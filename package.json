{
    "name": "frontend-template",
    "version": "1.0.0",
    "description": "",
    "author": "Andrii Lysenko",
    "homepage": "",
    "license": "ISC",
    "default": "src/index.html",
    "scripts": {
        "dev": "parcel serve src/index.html --open --port 3000 --watch-for-stdin",
        "run-container": "docker compose build && docker compose up",
        "build": "parcel build src/index.html --dist-dir dist",
        "clean": "rm -rf dist && rm -rf .parcel-cache",
        "check-types": "npx tsc --noEmit",
        "test": "npx jest",
        "test:watch": "npx jest --watch",
        "test:coverage": "npx jest --coverage",
        "lint": "npx eslint src/ --ext .ts,.tsx",
        "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
        "lint:styles": "stylelint \"./src/**/*.{js,jsx,ts,tsx}\"",
        "generate-locales": "node src/locales/generateLocales.js && eslint \"src/locales/locale.ts\" --fix --quiet"
    },
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@parcel/core": "^2.6.2",
        "@parcel/plugin": "^2.6.2",
        "@parcel/reporter-bundle-analyzer": "^2.6.2",
        "@parcel/transformer-typescript-tsc": "^2.6.2",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@types/i18n": "^0.13.3",
        "@types/jest": "^28.1.4",
        "@types/lodash": "^4.14.182",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/react-modal": "^3.13.1",
        "@types/react-select": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "autoprefixer": "^10.4.7",
        "eslint": "^8.18.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-parcel": "^1.10.6",
        "eslint-import-resolver-typescript": "^3.1.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.5.3",
        "eslint-plugin-jest-dom": "^4.0.2",
        "eslint-plugin-jsx-a11y": "^6.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-testing-library": "^5.5.1",
        "husky": "^8.0.1",
        "jest": "^28.1.2",
        "jest-environment-jsdom": "^28.1.2",
        "lint-staged": "^13.0.3",
        "parcel": "^2.6.2",
        "parcel-resolver-ts-base-url": "^1.1.5",
        "prettier": "^2.7.1",
        "process": "^0.11.10",
        "ts-jest": "^28.0.5",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@reduxjs/toolkit": "^1.8.3",
        "axios": "^0.27.2",
        "csstype": "^3.1.0",
        "graphql": "^16.5.0",
        "history": "^5.3.0",
        "i18next": "^21.8.11",
